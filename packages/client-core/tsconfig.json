{
  "extends": "@tambo-ai/typescript-config/base.json",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES6",
    "moduleResolution": "node",
    "declaration": true,
    "sourceMap": true,
    "inlineSources": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "jest.config.ts"],
  "exclude": ["node_modules", "./dist", "./esm"]
}
