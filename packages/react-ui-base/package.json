{
  "name": "@tambo-ai/react-ui-base",
  "version": "0.1.0-alpha.2",
  "description": "Headless base components for Tambo AI",
  "author": {
    "name": "Tambo",
    "url": "https://tambo.co"
  },
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./message": {
      "import": {
        "types": "./dist/esm/message/index.d.ts",
        "default": "./dist/esm/message/index.js"
      },
      "require": {
        "types": "./dist/cjs/message/index.d.ts",
        "default": "./dist/cjs/message/index.cjs"
      }
    },
    "./message-input": {
      "import": {
        "types": "./dist/esm/message-input/index.d.ts",
        "default": "./dist/esm/message-input/index.js"
      },
      "require": {
        "types": "./dist/cjs/message-input/index.d.ts",
        "default": "./dist/cjs/message-input/index.cjs"
      }
    },
    "./reasoning-info": {
      "import": {
        "types": "./dist/esm/reasoning-info/index.d.ts",
        "default": "./dist/esm/reasoning-info/index.js"
      },
      "require": {
        "types": "./dist/cjs/reasoning-info/index.d.ts",
        "default": "./dist/cjs/reasoning-info/index.cjs"
      }
    },
    "./toolcall-info": {
      "import": {
        "types": "./dist/esm/toolcall-info/index.d.ts",
        "default": "./dist/esm/toolcall-info/index.js"
      },
      "require": {
        "types": "./dist/cjs/toolcall-info/index.d.ts",
        "default": "./dist/cjs/toolcall-info/index.cjs"
      }
    },
    "./thread-history": {
      "import": {
        "types": "./dist/esm/thread-history/index.d.ts",
        "default": "./dist/esm/thread-history/index.js"
      },
      "require": {
        "types": "./dist/cjs/thread-history/index.d.ts",
        "default": "./dist/cjs/thread-history/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/types/index.js"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/types/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "check-types": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@tambo-ai/react": "*",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3"
  },
  "devDependencies": {
    "@tambo-ai/eslint-config": "*",
    "@tambo-ai/react": "*",
    "@tambo-ai/typescript-config": "*",
    "@tambo-ai/typescript-sdk": "^0.91.0",
    "@tambo-ai/vite-config": "*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.8",
    "@vitejs/plugin-react": "^4.5.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
