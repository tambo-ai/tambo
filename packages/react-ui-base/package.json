{
  "name": "@tambo-ai/react-ui-base",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./message": {
      "import": "./dist/message.js",
      "require": "./dist/message.cjs"
    },
    "./reasoning-info": {
      "import": {
        "types": "./dist/reasoning-info.d.ts",
        "default": "./dist/reasoning-info.js"
      },
      "require": {
        "types": "./dist/reasoning-info.d.ts",
        "default": "./dist/reasoning-info.cjs"
      }
    },
    "./toolcall-info": {
      "import": {
        "types": "./dist/toolcall-info.d.ts",
        "default": "./dist/toolcall-info.js"
      },
      "require": {
        "types": "./dist/toolcall-info.d.ts",
        "default": "./dist/toolcall-info.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.js"
      },
      "require": {
        "types": "./dist/types.d.ts",
        "default": "./dist/types.cjs"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "check-types": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "@tambo-ai/react": "*",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3"
  },
  "devDependencies": {
    "@tambo-ai/eslint-config": "*",
    "@tambo-ai/react": "*",
    "@tambo-ai/typescript-config": "*",
    "@tambo-ai/typescript-sdk": "^0.88.0",
    "@tambo-ai/vite-config": "*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.5",
    "@vitejs/plugin-react": "^4.5.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
