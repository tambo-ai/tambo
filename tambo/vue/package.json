{
  "name": "@tambo-ai/vue",
  "version": "0.1.0",
  "description": "Vue client package for Tambo AI",
  "repository": {
    "type": "git",
    "url": "https://github.com/tambo-ai/tambo.git",
    "directory": "tambo/vue"
  },
  "homepage": "https://tambo.co",
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./mcp": {
      "import": "./esm/mcp/index.js",
      "require": "./dist/mcp/index.js",
      "types": "./dist/mcp/index.d.ts"
    }
  },
  "files": [
    "dist",
    "esm"
  ],
  "engines": {
    "node": ">=22",
    "npm": ">=11"
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "check-types": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf dist esm coverage"
  },
  "peerDependencies": {
    "vue": "^3.4.0 || ^3.5.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@tambo-ai/typescript-sdk": "^0.70.0",
    "@tanstack/vue-query": "^5.85.5",
    "lodash-es": "^4.17.21",
    "partial-json": "^0.1.7",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "^5.1.5",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "^9.30.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "vite": "^5.4.10",
    "vitest": "^2.1.3",
    "vue": "^3.5.12"
  }
}

