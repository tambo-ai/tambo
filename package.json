  "scripts": {
    "tambo:setup": "bash ./scripts/cloud/tambo-setup.sh",
    "tambo:start": "bash ./scripts/cloud/tambo-start.sh",
    "tambo:stop": "bash ./scripts/cloud/tambo-stop.sh",
    "build": "turbo build",
    "build:sdk": "turbo build --filter=@tambo-ai/react",
