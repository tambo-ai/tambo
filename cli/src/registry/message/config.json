{
  "name": "message",
  "description": "Compound parts to render messages: Message, MessageContent, MessageImages, MessageRenderedComponentArea, ToolcallInfo",
  "componentName": "Message",
  "dependencies": [
    "class-variance-authority",
    "streamdown",
    "lucide-react",
    "highlight.js",
    "@tambo-ai/react",
    "@tambo-ai/typescript-sdk",
    "dompurify",
    "json-stringify-pretty-compact"
  ],
  "devDependencies": [],
  "requires": [],
  "files": [
    {
      "name": "message.tsx",
      "content": "src/registry/message/message.tsx"
    },
    {
      "name": "markdown-components.tsx",
      "content": "src/registry/message/markdown-components.tsx"
    },
    {
      "name": "lib/thread-hooks.ts",
      "content": "src/registry/lib/thread-hooks.ts"
    }
  ],
  "props": {
    "Message": { "role": { "type": "enum", "values": ["user", "assistant"] } },
    "MessageContent": { "markdown": { "type": "boolean", "default": true } },
    "MessageImages": {},
    "MessageRenderedComponentArea": {},
    "ToolcallInfo": {}
  },
  "usage": {
    "assistant": "<Message role=\"assistant\" message={msg}><MessageContent /><ToolcallInfo /></Message>",
    "userWithImages": "<Message role=\"user\" message={msg}><MessageImages /><MessageContent /></Message>"
  }
}
