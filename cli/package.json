{
  "name": "tambo",
  "version": "0.44.0",
  "description": "Tambo command-line tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/tambo-ai/tambo.git",
    "directory": "cli"
  },
  "homepage": "https://tambo.co",
  "module": "./dist/cli.js",
  "main": "./dist/cli.js",
  "bin": {
    "tambo": "./dist/cli.js"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    }
  },
  "files": [
    "src",
    "dist",
    "!**/*.test.*",
    "!**/__tests__/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "concurrently -c blue.bold,green.bold \"npm run dev:esm\"",
    "dev:esm": "tsc --watch",
    "check-types": "tsc --project tsconfig.check.json --noEmit",
    "test": "npm run test:esm && npm run test:react",
    "test:esm": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.ts",
    "test:react": "jest --config jest.config.react.ts",
    "format": "prettier --write .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format:lint": "npm run format && npm run lint:fix",
    "clean": "rimraf dist esm coverage"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=11"
  },
  "volta": {
    "extends": "../package.json"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tambo-ai/react": "*",
    "@tambo-ai/typescript-sdk": "^0.80.0",
    "chalk": "^5.6.0",
    "cli-table3": "^0.6.5",
    "clipboardy": "^5.0.0",
    "deepmerge": "^4.3.1",
    "diff": "^8.0.2",
    "dotenv": "^17.2.3",
    "fast-equals": "^5.3.3",
    "inquirer": "^12.9.4",
    "meow": "^14.0.0",
    "open": "^11.0.0",
    "ora": "^9.0.0",
    "react-markdown": "^10.1.0",
    "rehype-harden": "^1.1.7",
    "sanitize-html": "^2.17.0",
    "semver": "^7.7.3",
    "streamdown": "^1.6.10",
    "ts-morph": "^27.0.2"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.32",
    "@types/node-fetch": "^2.6.13",
    "@types/ora": "^3.2.0",
    "@types/recharts": "^2.0.1",
    "@types/semver": "^7.7.1",
    "jest": "^30.2.0",
    "memfs": "^4.51.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  }
}
