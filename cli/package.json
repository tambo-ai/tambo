{
  "name": "tambo",
  "version": "0.21.1",
  "description": "Tambo command-line tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/tambo-ai/tambo.git",
    "directory": "cli"
  },
  "homepage": "https://tambo.co",
  "module": "./dist/cli.js",
  "main": "./dist/cli.js",
  "bin": {
    "tambo": "./dist/cli.js"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "concurrently -c blue.bold,green.bold \"npm run dev:esm\"",
    "dev:esm": "tsc --watch",
    "check-types": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format:lint": "npm run format && npm run lint:fix",
    "clean": "rimraf dist esm coverage"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "volta": {
    "extends": "../package.json"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tambo-ai/react": "*",
    "@tambo-ai/typescript-sdk": "^0.61.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "clipboardy": "^4.0.0",
    "deep-equal": "^2.2.3",
    "deepmerge": "^4.3.1",
    "dotenv": "^17.0.0",
    "inquirer": "^10.2.2",
    "meow": "^13.2.0",
    "open": "^10.1.2",
    "ora": "^8.1.0",
    "sanitize-html": "^2.17.0",
    "semver": "^7.7.2",
    "ts-morph": "^26.0.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.4",
    "@types/deep-equal": "^1.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.15.32",
    "@types/node-fetch": "^2.6.12",
    "@types/ora": "^3.2.0",
    "@types/recharts": "^1.8.29",
    "@types/semver": "^7.7.0",
    "typescript": "^5.8.3"
  }
}
