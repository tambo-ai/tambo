{
  "name": "tambo",
  "version": "0.0.1",
  "description": "Tambo command-line tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/tambo-ai/tambo.git",
    "directory": "cli"
  },
  "homepage": "https://tambo.co",
  "module": "./dist/cli.js",
  "main": "./dist/cli.js",
  "bin": {
    "tambo": "./dist/cli.js"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "concurrently -c blue.bold,green.bold \"npm run dev:esm\"",
    "dev:esm": "tsc --watch",
    "check-types": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format:lint": "npm run format && npm run lint:fix",
    "clean": "rimraf dist esm coverage"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@tambo-ai/react": "*",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "deep-equal": "^2.2.3",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.4.5",
    "inquirer": "^9.1.4",
    "meow": "^13.2.0",
    "open": "^10.1.0",
    "ora": "^8.1.0",
    "sanitize-html": "^2.14.0",
    "ts-json-schema-generator": "^2.3.0",
    "ts-morph": "^25.0.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/deep-equal": "^1.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20",
    "@types/ora": "^3.1.0",
    "@types/recharts": "^1.8.29",
    "@types/uuid": "^10.0.0",
    "execa": "^8.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
