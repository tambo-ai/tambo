{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "es6"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [{ "name": "next" }],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@modelcontextprotocol/sdk": [
        "./node_modules/@modelcontextprotocol/sdk/dist"
      ],
      "@modelcontextprotocol/sdk/*": [
        "./node_modules/@modelcontextprotocol/sdk/dist/*"
      ],
      "@modelcontextprotocol/sdk/client/index.js": [
        "./node_modules/@modelcontextprotocol/sdk/dist/client/index.js"
      ],
      "@modelcontextprotocol/sdk/client/auth.js": [
        "./node_modules/@modelcontextprotocol/sdk/dist/client/auth.js"
      ],
      "@modelcontextprotocol/sdk/client/sse.js": [
        "./node_modules/@modelcontextprotocol/sdk/dist/client/sse.js"
      ],
      "@modelcontextprotocol/sdk/client/streamableHttp.js": [
        "./node_modules/@modelcontextprotocol/sdk/dist/client/streamableHttp.js"
      ],
      "@modelcontextprotocol/sdk/shared/protocol.js": [
        "./node_modules/@modelcontextprotocol/sdk/dist/shared/protocol.js"
      ]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
